@{
    ViewBag.Title = "Error";
}
@if (Request.IsAuthenticated)
{
    string navbar = "_" + ViewBag.UserType + "Navbar";
    @Html.Partial(navbar)
}
else
{
    @Html.Partial("_EmployeeNavbar")
}
<div class="container content center-vertical">
    <div class="row">
        <div class="col-md-10 offset-md-1 text-danger" style="opacity:.3;">
            <i class="fas fa-exclamation-circle fa-7x mb-2"></i>
            <h2>Server side error occured while processing your request</h2>
            <h3>Please try again later.</h3>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}