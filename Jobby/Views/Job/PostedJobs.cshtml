@model IEnumerable<Jobby.Models.Job>
@{ ViewBag.Title = "Posted Jobs"; }
@section style
{
    <link rel="stylesheet" href="~/Content/jobs.css" />
    <link rel="stylesheet" href="~/Content/posted-jobs.css" />
}

@Html.Partial("_EmployerNavbar")

<div class="container content">
    <div class="row">
        <div class="col-lg-10 offset-lg-1">
            <div class="text-right my-3">
                <a class="btn btn-outline-primary" href=@Url.Action("Create","Job")> <i class="fas fa-plus-circle"></i> Create New</a>
            </div>
            <div id="jobs-container">
                @Html.Partial("_PostedJobs")
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/job.js"></script>
}

